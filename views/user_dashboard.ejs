<h3>User Dashboard</h3>
<div class="row g-3 mb-3">
  <div class="col-md-6"><div class="card p-3"><h5>Total Links</h5><p><%= stats.totalLinks %></p></div></div>
  <div class="col-md-6"><div class="card p-3"><h5>Total Clicks</h5><p><%= stats.totalClicks %></p></div></div>
</div>

<div class="card p-3 mb-3">
  <h5>Last 5 Clicks</h5>
  <table class="table table-sm">
    <thead><tr><th>Time</th><th>Short</th><th>IP</th><th>Country</th><th>City</th></tr></thead>
    <tbody>
      <% lastClicks.forEach(c=>{ %>
      <tr>
        <td><%= c.createdAt.toLocaleString() %></td>
        <td><a href="/r/<%= c.Url.shortCode %>"><%= c.Url.shortCode %></a></td>
        <td><%= c.ip %></td>
        <td><%= c.country||'-' %></td>
        <td><%= c.city||'-' %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>
